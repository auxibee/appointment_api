{
  "name": "appointment_api",
  "version": "1.0.0",
  "description": "A simple appointment webservice nodejs, express, postgres",
  "main": "bin/www.js",
  "scripts": {
    "start": "node bin/www.js",
    "dev": "nodemon www/bin.js",
    "test": " SET NODE_ENV=test&& mocha tests/**/*test.js --reporter spec --grep",
    "lint": "eslint",
