{
  "name": "appointment_api",
  "version": "1.0.0",
  "description": "A simple appointment webservice nodejs, express, postgres",
  "main": "bin/www.js",
  "scripts": {
    "start": "node bin/www.js",
    "dev": "nodemon www/bin.js",
    "migrate-testdb": "npx sequelize db:migrate --env=test",
    "drop-testdb": "npx sequelize db:migrate:undo:all --env=test",
    "test-setup": "cross-env NODE_ENV=test mocha tests/**/*test.js --reporter spec --grep",
    "test": "npm run drop-testdb  && npm run migrate-testdb && npm run test-setup",
    "lint": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auxibee/appointment_api.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "postgres",
    "api",
    "sql"
  ],
  "author": "Yaw Twumasi Acheampong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/auxibee/appointment_api/issues"
  },
  "homepage": "https://github.com/auxibee/appointment_api#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "sequelize": "^6.34.0",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "umzug": "^3.4.0"
  }
}
